-- zenscript
-- Load Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/stysscythe/script/main/LibTest.lua"))()

-- Create Main Window
local Window = Library.Window('Zenscript UI')

-- Create Tabs
local BadTab = Window.CreateTab('Bad')
local GoodTab = Window.CreateTab('Good')

-- =====================
-- BAD TAB (Hacks etc.)
-- =====================
BadTab.CreateDivider("Fun but risky stuff")

-- ESP Toggle
BadTab.CreateToggle("ESP", function(state)
    if state then
        print("ESP Enabled")
    else
        print("ESP Disabled")
    end
end)

-- No Clip Toggle
BadTab.CreateToggle("No Clip", function(state)
    if state then
        print("No Clip Enabled")
    else
        print("No Clip Disabled")
    end
end)

-- Speed Slider
BadTab.CreateSlider("Speed", 16, 100, function(speed)
    print("Walkspeed set to: " .. speed)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
end)

-- Button Example
BadTab.CreateButton("Crash Yourself", function()
    print("This would run a crash function here.")
end)

-- =====================
-- GOOD TAB (Helpful stuff)
-- =====================
GoodTab.CreateDivider("Safe utilities")

-- Rejoin Button
GoodTab.CreateButton("Rejoin Server", function()
    local ts = game:GetService("TeleportService")
    local p = game.Players.LocalPlayer
    ts:Teleport(game.PlaceId, p)
end)

-- FPS Toggle (prints only, placeholder)
GoodTab.CreateToggle("Show FPS", function(state)
    if state then
        print("FPS Display On")
    else
        print("FPS Display Off")
    end
end)

-- Jump Power Slider
GoodTab.CreateSlider("Jump Power", 50, 200, function(power)
    print("Jump Power set to: " .. power)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = power
end)

-- Chat Logger Button
GoodTab.CreateButton("Enable Chat Logger", function()
    print("Chat logger enabled (placeholder).")
end)

-- =====================
-- Universal Controls
-- =====================
GoodTab.CreateKeybind("Toggle UI", Enum.KeyCode.RightAlt, function()
    Library:ToggleUI()
end)
